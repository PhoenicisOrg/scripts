{
    "opts" : {
        "destination" : "./docs/jsdoc/",
        "recurse"     : true
    },
    "plugins"      : [],
    "recurseDepth" : 10,
    "source"       : {
        "exclude": ["docs"],
        "includePattern" : ".+\\.js(doc|x)?$",
        "excludePattern" : "(^|\\/|\\\\)_"
    },
    "sourceType" : "module",
    "tags"       : {
        "allowUnknownTags" : true,
        "dictionaries"     : [
            "jsdoc",
            "closure"
        ]
    },
    "templates" : {
        "default" : {
            "includeDate" : false
        }
    }
}
